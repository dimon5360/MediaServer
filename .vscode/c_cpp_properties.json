{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/core/**",
                "${workspaceFolder}/net/**",
                "${workspaceFolder}/config/**",
                "${workspaceFolder}/core"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c23",
            "cppStandard": "c++23",
            "intelliSenseMode": "linux-clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/core/**",
                    "${workspaceFolder}/net/**",
                    "${workspaceFolder}/config/**",
                    "/usr/local/include/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}